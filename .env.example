# Environment configuratie voor Vercel deployment

# Production domain (via Cloudflare proxy)
VITE_PRODUCTION_URL=https://zwijsen.net

# Vercel direct URL (fallback)
VITE_VERCEL_URL=https://zwijsen-regio.vercel.app

# WordPress API endpoints (indien nodig voor content sync)
VITE_WORDPRESS_API=https://zwijsen.net/wp-json/wp/v2

# Analytics (optioneel)
VITE_GA_ID=
VITE_GTM_ID=

# Feature flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_INQUIRY_FORM=true
